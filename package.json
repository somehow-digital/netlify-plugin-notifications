{
	"name": "@somehow-digital/netlify-plugin-notifications",
	"type": "module",
	"version": "0.0.0",
	"packageManager": "pnpm@10.25.0",
	"description": "Netlify plugin to send notifications.",
	"license": "MIT",
	"repository": "somehow-digital/netlify-plugin-notifications",
	"keywords": [
		"netlify-plugin",
		"netlify"
	],
	"exports": {
		".": "./src/index.js"
	},
	"files": [
		"./manifest.yml",
		"./src/"
	],
	"engines": {
		"node": ">=22",
		"pnpm": ">=10"
	},
	"scripts": {
		"prepare": "concurrently --group --prefix-colors 'auto' 'pnpm:prepare:*'",
		"prepare:husky": "husky",
		"lint": "concurrently --group --prefix-colors 'auto' 'pnpm:lint:*'",
		"lint:eslint": "eslint .",
		"lint:cspell": "cspell --no-progress --no-summary --no-must-find-files .",
		"lint:knip": "knip --no-progress",
		"fix": "concurrently --group --prefix-colors 'auto' 'pnpm:fix:*'",
		"fix:eslint": "eslint --fix .",
		"fix:knip": "knip --no-progress --fix"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.0.0",
		"@somehow-digital/commitlint-config": "^1.0.0",
		"@somehow-digital/cspell-dictionary": "^2.0.0",
		"@somehow-digital/eslint-config": "^4.0.0",
		"@somehow-digital/semantic-release-config": "^2.0.0",
		"concurrently": "^9.0.0",
		"cspell": "^9.0.0",
		"eslint": "^9.3.0",
		"husky": "^9.0.11",
		"knip": "^5.16.0",
		"lint-staged": "^16.0.0",
		"semantic-release": "^25.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"oxc-resolver"
		]
	}
}
